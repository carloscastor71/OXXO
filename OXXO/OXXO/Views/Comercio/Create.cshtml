


@{
    ViewData["Title"] = "Alta de comercios";
}
<h1>Alta</h1>

<hr />
<div class="container-fluid">
    <form asp-action="Create">
        <div class="row">
            <div class="col">
                <div class="form-group col-6">
                    <div class="mb-3">
                        <label class="control-label"></label>
                        <input value="Datos generales" class="form-control" disabled />
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-4">
                <div class="form-group col-6">
                    <div class="mb-3">
                        <label class="control-label">ID Emisor</label>
                        <input value="Se genera automaticamente" class="form-control  " disabled />
                    </div>
                </div>
            </div>
            <div class="col-2">
                <div class="form-group col-12">
                    <div class="mb-3">
                        <label class="control-label">Tipo persona</label>
                        <input class="form-control  " />
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <div class="form-group col-6">
                    <div class="mb-3">
                        <label class="control-label">Razon social</label>
                        <input class="form-control  " />
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <div class="form-group col-6">
                    <div class="mb-3">
                        <label class="control-label">Nombre comercial</label>
                        <input class="form-control  " />
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <div class="form-group col-6">
                    <div class="mb-3">
                        <label class="control-label">Direccion</label>
                        <input class="form-control  " />
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-2">
                <div class="form-group col-12">
                    <div class="mb-3">
                        <label class="control-label">Giro</label>
                        <input class="form-control" />
                    </div>
                </div>
            </div>
            <div class="col-2">
                <div class="form-group col-12">
                    <div class="mb-3">
                        <label class="control-label">RFC</label>
                        <input class="form-control" />
                    </div>
                </div>
            </div>
            <div class="col-2">
                <div class="form-group col-12">
                    <div class="mb-3">
                        <label class="control-label">Telefono</label>
                        <input class="form-control" />
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <div class="form-group col-6">
                    <div class=" mb-3">
                        <label class="control-label">Portal</label>
                        <input class="form-control  " />
                        <span class="text-danger"></span>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col">
                <div class="form-group col-6">
                    <div class=" mb-3">
                        <label class="control-label">Correo</label>
                        <input class="form-control  " />
                        <span class="text-danger"></span>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-2">
                <div class="form-group col-12">
                    <div class="mb-3">
                        <label class="control-label">Cuenta</label>
                        <input class="form-control" />
                    </div>
                </div>
            </div>
            <div class="col-2">
                <div class="form-group col-12">
                    <div class="mb-3">
                        <label class="control-label">Banco</label>
                        <input class="form-control" />
                    </div>
                </div>
            </div>
            <div class="col-2">
                <div class="form-group col-12">
                    <div class="mb-3">
                        <label class="control-label">Tipo de deposito</label>
                        <input class="form-control" />
                    </div>
                </div>
            </div>
        </div>
        <div class="">
            <div class="row">
                <div class="form-group col-1">
                    <a class="btn btn-outline-danger" asp-action="Index">Cancelar</a>
                </div>
                <div class="form-group col-1">
                    <input type="submit" value="Dar de alta" class="btn btn-outline-success" />
                </div>
            </div>
        </div>
    </form>

</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}